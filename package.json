{
	"name": "vite-boilerplate",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome lint --write .",
		"format": "biome format --write .",
